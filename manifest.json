{
  "manifest_version": 3,
  "name": "看看",
  "version": "22.08.152",
  "description": "看看插件",
  "icons": {
    "16": "img/20x20.png",
    "48": "img/40x40.png",
    "128": "img/120x120.png"
  },
  "background": {
    "service_worker": "./js/background.js"
  },
  "action": {
    "default_icon": "img/20x20.png",
    "default_title": "看看插件",
    "default_popup": "html/popup.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["./css/inject.css"],
      "js": ["./js/inject.js"],
      "run_at": "document_end"
    }
  ],
  "homepage_url": "https://xuehuayu.cn"
}